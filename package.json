{
  "name": "latenighteats",
  "version": "1.0.0",
  "description": "A directory & map of the best eateries that open 'late' in San Francisco (you're welcome!)",
  "scripts": {
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/hjs-dev-server",
    "clean": "rimraf dist",
    "build": "npm run clean && ./node_modules/.bin/cross-env NODE_ENV=production webpack",
    "publish_pages": "gh-pages -d dist",
    "ghpages": "npm run build && npm run publish_pages",
    "test": "cross-env NODE_ENV=test ./node_modules/karma/bin/karma start karma.conf.js",
    "test:watch": "npm run test -- --watch"
  },
